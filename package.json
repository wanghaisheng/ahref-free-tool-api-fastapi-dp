{
  "name": "ahref-api-vercel",
  "version": "1.0.0",
  "scripts": {
    "setup": "npm install",
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "next-dev": "npm install && next dev --experimental-https",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "npm run dev && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "description": "Simple ahref api  using Playwright hosted as a serverless function on Vercel",
  "main": "index.js",
  "author": "wanghaisheng",
  "license": "MIT",
  "dependencies": {
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@sparticuz/chromium-min": "123.0.0",
    "playwright-core": "1.42.0",
    "@sparticuz/chromium": "^123.0.1",
    "puppeteer": "^22.6.2",
    "puppeteer-core": "^21.3.8",
    "semantic-release": "^21.0.5",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "@types/node": "^20.3.2"
  }
}